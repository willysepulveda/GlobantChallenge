{
	"name": "DS_ExcelPoc",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_sablpoc",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"table": {
				"type": "string"
			},
			"sheet": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"sheetName": {
				"value": "@dataset().sheet",
				"type": "Expression"
			},
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().table,'.xlsx')",
					"type": "Expression"
				},
				"container": "pocglobant"
			}
		},
		"schema": []
	}
}